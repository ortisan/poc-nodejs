  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build-web": "nest build web",
    "start-web": "nest start web",
    "start-web:dev": "nest start web --watch",
    "start-web:debug": "nest start web --debug --watch",
    "start-web:prod": "node dist/apps/web/main",
    "build-audit-function": "nest build audit-function",
    "debug-audit-function": "nest start audit-function --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/poc-nodejs/test/jest-e2e.json",
    "commitlint": "commitlint --edit"
  },
